@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";
}
<h1>Index Category</h1>
<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                ID
            </th>
            <th>
                Name
            </th>
            <th>
                Order
            </th>
            <th>
                Date
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
                <tr>
                    <td width="20%">
                        @obj.Id
                    </td>
                    <td width="50%">
                        @obj.Name
                    </td>
                    <td width="30%">
                        @obj.DisplayOrder
                    </td>
                    <td width="50%">
                        @obj.CreateDateTime
                    </td>
                </tr>
        }
    </tbody>
</table>
